<div class="container">
 
    <h1 align="center">Login</h1>
    <h2></h2>
    

    <div align="center" color="#0066CC">


<%= form_tag sessions_path do %>
  <%= label_tag :email %><br/>
  <%= text_field_tag :email, params[:email] %><br/>
  <%= label_tag :password %><br/>
  <%= password_field_tag :password %><br/>
  <%= submit_tag "Log In" %>
<% end %>
</div>

  </div>

